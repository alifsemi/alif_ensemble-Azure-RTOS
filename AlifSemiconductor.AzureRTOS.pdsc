      <!-- Arm architecture -->
  <conditions>
    <condition id="ARMCC GCC IAR">
      <accept Tcompiler="ARMCC"/>
      <accept Tcompiler="GCC"/>
      <accept Tcompiler="IAR"/>
    </condition>

    <condition id="Alif M55 Device">
      <description>Alif Semiconductor Armv8-M architecture based device</description>
      <accept Dvendor="Alif Semiconductor:165" Dcore="Cortex-M55"/>
    </condition>

    <condition id="RTOS AzureRTOS">
      <description>Components required for AzureRTOS</description>
      <require condition="Alif M55 Device"/>
      <require condition="ARMCC GCC IAR"/>
      <require Cclass="Device" Cgroup="Startup"/>
      <deny    Cclass="CMSIS"  Cgroup="RTOS2" Csub="Keil RTX5"/>
    </condition>

    <condition id="AzureRTOS FILEX">
      <description>Components required for AzureRTOS FILEX</description>
      <require condition="RTOS AzureRTOS"/>
      <require Cclass="RTOS" Cgroup="Kernel"/>
    </condition>

    <condition id="AzureRTOS FILEX SD DRIVER">
      <description>Components required for AzureRTOS FILEX SD Driver</description>
      <require Cclass="RTOS" Cgroup="FILEX" Csub="Core"/>
    </condition>

    <condition id="AzureRTOS NETX">
      <description>Components required for AzureRTOS NETX</description>
      <require condition="RTOS AzureRTOS"/>
      <require Cclass="RTOS" Cgroup="Kernel"/>
    </condition>

    <condition id="AzureRTOS USBX">
      <description>Components required for AzureRTOS USBX</description>
      <require condition="RTOS AzureRTOS"/>
      <require Cclass="RTOS" Cgroup="Kernel"/>
    </condition>

  </conditions>



