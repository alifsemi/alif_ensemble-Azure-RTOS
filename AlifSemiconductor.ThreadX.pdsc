      <!-- Arm architecture -->
  <conditions>
    <!--condition id="ARMCC GCC IAR">
      <accept Tcompiler="ARMCC"/>
      <accept Tcompiler="GCC"/>
      <accept Tcompiler="IAR"/>
    </condition-->
     <condition id="Startup ARM">
      <require Tcompiler="ARMCC"/>
    </condition>
    <condition id="Startup GCC">
      <require Tcompiler="GCC"/>
    </condition>
    <condition id="Alif M55 Device">
      <description>Alif Semiconductor Armv8-M architecture based device</description>
      <accept Dvendor="Alif Semiconductor:165" Dcore="Cortex-M55"/>
    </condition>

    <condition id="RTOS ThreadX">
      <description>Components required for ThreadX</description>
      <require condition="Alif M55 Device"/>
      <!--require condition="ARMCC"/-->
      <accept condition="Startup ARM"/>
      <accept condition="Startup GCC"/>
      <require Cclass="Device" Cgroup="Startup"/>
      <deny    Cclass="CMSIS"  Cgroup="RTOS2" Csub="Keil RTX5"/>
    </condition>

    <condition id="ThreadX FILEX">
      <description>Components required for ThreadX FILEX</description>
      <require condition="RTOS ThreadX"/>
      <require Cclass="RTOS" Cgroup="Kernel"/>
    </condition>

    <condition id="ThreadX FILEX SD DRIVER">
      <description>Components required for ThreadX FILEX SD Driver</description>
      <require Cclass="RTOS" Cgroup="FILEX" Csub="Core"/>
    </condition>

    <condition id="ThreadX NETX">
      <description>Components required for ThreadX NETX</description>
      <require condition="RTOS ThreadX"/>
      <require Cclass="RTOS" Cgroup="Kernel"/>
    </condition>

    <condition id="ThreadX USBX">
      <description>Components required for ThreadX USBX</description>
      <require condition="RTOS ThreadX"/>
      <require Cclass="RTOS" Cgroup="Kernel"/>
    </condition>

    <condition id="USBX Host Device">
      <description>Components required for ThreadX USBX Host Device</description>
      <require condition="ThreadX USBX"/>
      <require Cclass="RTOS" Cgroup="USBX" Csub="Core"/>
    </condition>

    <condition id="E8">
      <description>Alif Semiconductor E8 device</description>
      <require Dvendor="Alif Semiconductor:165"/>
      <accept Pname="M55_HE" Dname="AE822FA0*"/>
      <accept Pname="M55_HP" Dname="AE822FA0*"/>
      <accept Pname="M55_HE" Dname="AE402FA0*"/>
      <accept Pname="M55_HP" Dname="AE402FA0*"/>
    </condition>

    <condition id="E7">
    <description>Alif Semiconductor E7 and other device</description>
      <require Dvendor="Alif Semiconductor:165"/>
      <accept Pname="M55_HE" Dname="AE722F80*"/>
      <accept Pname="M55_HP" Dname="AE722F80*"/>
      <accept Pname="M55_HE" Dname="AE512F80*"/>
      <accept Pname="M55_HP" Dname="AE512F80*"/>
      <accept Pname="M55_HE" Dname="AE302F*"/>
      <accept Pname="M55_HP" Dname="AE302F*"/>
      <accept Pname="M55_HE" Dname="AE101F*"/>
      <accept Pname="M55_HE" Dname="AE1C1F*"/>
    </condition>

  </conditions>



